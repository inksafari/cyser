Host *
    Compression         yes
    ControlMaster       auto
    ControlPath         ~/.ssh/sockets/%r@%h:%p
    ControlPersist      600
    ForwardAgent        yes
    Protocol            2
    ServerAliveInterval 300
    TCPKeepAlive        yes

#
# Multiple GitHub accounts
#
Host git-inksafari
    HostName github.com
    User inksafari
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/id_rsa_inksafari_github

